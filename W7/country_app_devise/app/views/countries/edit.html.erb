<%= form_for @country do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :population %>
    <%= f.text_field :population %>
    <br>
    <%= f.label :language %>
    <%= f.text_field :language %>
    <br>
    <%= f.label :currency %>
    <%= f.text_field :currency %>
    <br>
    <%= f.label :image %>
    <%= f.url_field :image %>
    <br>
    <%= f.submit 'Input country!' %>
  <% end %>